{
  "name": "mongo-request-bin",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "docs": "npm run docs:html && npm run docs:markdown",
    "docs:html": "./node_modules/.bin/apidoc -i routes -o docs",
    "docs:markdown": "./node_modules/.bin/apidoc-markdown -p docs -o README.md"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.9",
    "express": "^4.16.4",
    "mongoose": "^5.1.6",
    "morgan": "^1.9.1",
    "request": "^2.88.0",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0"
  },
  "devDependencies": {
    "apidoc-markdown": "^0.2.1",
    "cz-customizable": "5.3.0"
  }
}